{
  "name": "mukinho",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.2.2",
      "zod": "^4.1.13",
      "typescript": "^5",
      "@trpc/server": "^11.7.2",
      "@trpc/client": "^11.7.2",
      "@libsql/client": "0.15.15",
      "libsql": "0.5.22",
      "better-auth": "^1.4.7"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
